{
  "expo": {
    "name": "jireh-app",
    "slug": "jireh-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/jireh.jpg",
    "userInterfaceStyle": "light",
    "scheme": "jireh",
    "splash": {
      "image": "./assets/jirehslider.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "newArchEnabled": true,
      "bridgelessEnabled": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": true,
      "bridgelessEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"]
  }
}
